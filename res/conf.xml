<?xml version="1.0" encoding="UTF-8" ?>

<!--
	Only the first database, and the first auths of each type are used.
	Proceeding tags are ignored.
	Do not have empty element nodes as this will break things.
-->

<config version="1.2" >

	<database>
		<host>host</host>
		<port>5432</port>
		<db>db</db>
	</database>

	<auth>
		<user>rouser</user>
		<pass>ropass</pass>
		<type>read_only</type>
	</auth>
	
	<auth>
		<user>rwuser</user>
		<pass>rwpass</pass>
		<type>read_write</type>
	</auth>
</config>
